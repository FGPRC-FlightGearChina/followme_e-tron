    <flight_control name="Truck">

        <channel name="Steer">
            <summer name="Steer Sum">
                <input>fcs/rudder-cmd-norm</input>
                <clipto>
                    <min>-1</min>
                    <max>1</max>
                </clipto>
                <output>fcs/rudder-pos-norm</output>
            </summer>
            <lag_filter name="fcs/rudder-pos-norm">
				<input>fcs/rudder-cmd-norm</input>
				<c1>30</c1>
			</lag_filter>
        </channel>
    </flight_control>

    <aerodynamics>
        <axis name="DRAG">
            <function name="aero/coefficient/CD">
                <product>
                    <property>aero/qbar-area</property>
                    <value>0.8</value>
                </product>
            </function>
        </axis>
        <axis name="SIDE"/>
        <axis name="ROLL"/>
        <axis name="PITCH"/>
        <axis name="YAW"/>
    </aerodynamics>

